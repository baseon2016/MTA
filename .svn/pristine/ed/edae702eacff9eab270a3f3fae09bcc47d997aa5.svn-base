body {
  margin: 0;
  border: 0;
  font-family: Microsoft YaHei;
}
* {
  box-sizing: border-box;
}
a {
  text-decoration: none;
}
ul {
  list-style: none;
  padding: 0;
  margin: 0;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
}
input,
select {
  outline: 0;
  border: 0;
}

[v-cloak] {
  display: none !important;
}
p {
  margin: 0;
}
.df {
  display: flex;
}
.dn {
  display: none;
}
.container {
  width: 1200px;
  margin: 0 auto;
}
.main-bg {
  background-color: #003262;
}
.sub-bg {
  background-color: #002856;
}
.fff-bg {
  background-color: #fff;
}
.main-title {
  font-size: 20px;
  font-weight: bold;
  color: #fff;
  line-height: 1.7;
}
.min-h-744 {
  min-height: 744px;
}
.min-h-330 {
  min-height: 330px;
}
.mg-t-20 {
  margin-top: 20px;
}
.mg-t-40 {
  margin-top: 40px;
}
.mg-b-32 {
  margin-bottom: 32px !important;
}
.mg-r-30 {
  margin-right: 30px;
}
.mg-r-120 {
  margin-right: 120px;
}
.mg-l-20 {
  margin-left: 20px;
}
.mg-l-40 {
  margin-left: 40px;
}
.mg-0 {
  margin: 0 auto;
}
.po-r {
  position: relative;
}
.po-a {
  position: absolute;
}
.border-bo-1 {
  border-bottom: 1px solid #ededed;
}
/*element 弹窗样式修改 */
.el-message-box__wrapper .el-message-box__title {
  color: #333;
}
.el-message {
  font-size: 14px;
}
.el-message.el-message--success .el-icon-success {
  color: #2a4793;
}
.el-message.el-message--success .el-message__content {
  color: #2a4793;
}
.el-message.el-message--success {
  background-color: #ecf5ff;
  border-color: #ecf5ff;
}
/*element 按钮的基础|激活|焦点|悬停样式设置  */
.el-button.el-button--primary:focus,
.el-button.el-button--primary {
  color: #fff;
  background-color: #2a4793;
  border-color: #2a4793;
}

.el-button.el-button--primary:hover {
  background: #5272c6;
  border-color: #5272c6;
  color: #fff;
}
.el-button.el-button--primary.is-active,
.el-button.el-button--primary:active {
  color: #fff;
  background-color: #2a4793;
  border-color: #2a4793;
}
button.el-button {
  background: #fff;
  border-color: #dcdfe6;
  color: #606266;
}
button.el-button:focus,
button.el-button:hover {
  color: #98aee1;
  border-color: #98aee1;
  background-color: #ecf5ff;
}
button.el-button.is-active,
button.el-button:active {
  background: #fff;
  border-color: #dcdfe6;
  color: #606266;
}
